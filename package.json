		"@types/uuid": "^3.4.4",
		"babel-jest": "^24.7.1",
		"jest": "^24.7.1",
		"metro-react-native-babel-preset": "^0.53.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.16.4",
